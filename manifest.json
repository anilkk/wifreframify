{
  "manifest_version": 2,

  "name": "wireframify",
  "description": "view wireframe of the page",
  "version": "1.0.1",

  "browser_action": {
    "default_icon": {
        "19": "icon19.png",
        "38": "icon38.png"
    },
    "default_title": "view wireframe of the page"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "web_accessible_resources": ["jquery.min.js","framify.js"]
}
